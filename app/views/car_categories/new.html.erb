<%= form_for @car_category do |f|%>
  <%= f.label :name, 'Nome'%>
  <%= f.text_field :name%>
  <%= f.label :daily_rate, 'Diaria'%>
  <%= f.text_field :daily_rate%>
  <%= f.label :car_insurance, 'Seguro'%>
  <%= f.text_field :car_insurance%>
  <%= f.label :third_party_insurance, 'Terceira parte do seguro'%>
  <%= f.text_field :third_party_insurance%>
  <%= f.submit 'Enviar'%>
<%end%>